{
  "name": "konekte",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "env-cmd -f .env.development vite",
    "start:dev": "env-cmd -f .env.development vite",
    "start:prod": "env-cmd -f .env.production vite",
    "build:dev": "env-cmd -f .env.development tsc && vite build && aws s3 sync ./dist/ s3://konekte.com.br  --acl public-read",
    "build:prod": "env-cmd -f .env.production tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "formik": "^2.4.2",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-currency-input-field": "^3.6.11",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-input-mask": "^2.0.4",
    "react-modal": "^3.16.1",
    "react-number-format": "^5.2.2",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.0.7",
    "ts-jest": "^29.1.1",
    "vite-plugin-svgr": "^3.2.0",
    "yup": "^1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier",
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.2",
    "@svgr/rollup": "^8.0.1",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.37",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
